  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="RowID">
          <DataField>RowID</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Asset_Type_Name">
          <DataField>Asset_Type_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Asset_Name">
          <DataField>Asset_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="All_Amount">
          <DataField>All_Amount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Budget_Year">
          <DataField>Budget_Year</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Receive_from">
          <DataField>Receive_from</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Amount">
          <DataField>Amount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT E.RowID,
		A.[Asset_Type_Name],
		A.[Asset_Name],
		E.[Amount] as [All_Amount],
		A.[Amount],
		A.Budget_Year,
		A.[Receive_from]
FROM (
	SELECT C.Asset_Type_Name_T  as [Asset_Type_Name]
	  ,D.Asset_Name1_T as [Asset_Name]   
	  ,COUNT(*) as [Amount]
	  ,A.[Budget_Year]
	  ,A.[Receive_from]
	  ,A.Asset_ID
	  ,A.Asset_Type_ID
	FROM [MS_Asset_Detail] A
	LEFT JOIN [MS_Work_Center] B ON B.KeyID = A.Loc_ID
	LEFT JOIN [MS_Asset_Type] C ON C.KeyID = A.Asset_Type_ID
	LEFT JOIN [MS_Asset] D ON D.KeyID = A.Asset_ID
	--WHERE (A.Loc_ID = @Loc_ID OR @Loc_ID = '') AND A.[Budget_Year] = @Year
	GROUP BY D.Asset_Name1_T,C.Asset_Type_Name_T,A.[Budget_Year],A.[Receive_from],A.Asset_ID ,A.Asset_Type_ID
) A
LEFT JOIN (
	SELECT ROW_NUMBER() OVER (ORDER BY C.Asset_Type_Name_T desc,D.Asset_Name1_T desc) AS RowID  
	  ,D.KeyID as [Asset_ID]
	  ,C.KeyID as [Asset_Type_ID]
	  ,COUNT(*) as [Amount] 
	FROM [MS_Asset_Detail] A
	LEFT JOIN [MS_Work_Center] B ON B.KeyID = A.Loc_ID
	LEFT JOIN [MS_Asset_Type] C ON C.KeyID = A.Asset_Type_ID
	LEFT JOIN [MS_Asset] D ON D.KeyID = A.Asset_ID
	GROUP BY D.KeyID,C.KeyID, C.Asset_Type_Name_T ,D.Asset_Name1_T
) E ON E.[Asset_ID] = A.[Asset_ID] and E.[Asset_Type_ID] = A.Asset_Type_ID
WHERE E.RowID is not null
ORDER BY E.RowID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
